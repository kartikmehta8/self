{
  "name": "@selfxyz/qrcode-angular",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/selfxyz/self"
  },
  "license": "MIT",
  "author": "turnoffthiscomputer",
  "description": "Angular QR code wrapper SDK for Self passport verification",
  "keywords": [
    "angular",
    "qrcode",
    "self",
    "passport",
    "verification",
    "identity"
  ],
  "main": "bundles/selfxyz-qrcode-angular.umd.js",
  "module": "fesm2022/selfxyz-qrcode-angular.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "build:watch": "ng-packagr -p ng-package.json --watch",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write .",
    "nice": "yarn format && yarn lint",
    "prepublishOnly": "yarn build",
    "publish": "yarn npm publish --access public"
  },
  "dependencies": {
    "angularx-qrcode": "^18.0.2",
    "js-sha1": "^0.7.0",
    "js-sha256": "^0.11.0",
    "js-sha512": "^0.9.0",
    "lottie-web": "^5.12.2",
    "node-forge": "^1.3.1",
    "poseidon-lite": "^0.3.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@types/node": "^22.0.0",
    "@types/node-forge": "^1.3.5",
    "@types/uuid": "^10.0.0",
    "ng-packagr": "^18.0.0",
    "prettier": "^3.5.3",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.0",
    "typescript": "~5.5.4",
    "zone.js": "^0.14.0"
  },
  "peerDependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@selfxyz/common": "workspace:^",
    "rxjs": "^7.8.0"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "publishConfig": {
    "access": "public"
  }
}
