{
  "name": "@selfxyz/ts-api-test",
  "version": "1.0.0",
  "description": "TypeScript API for testing SelfBackendVerifier",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch server.ts",
    "start": "node dist/server.js",
    "test-endpoint": "node ./test-verify-endpoint.js",
    "gen-mock-proof": "tsx genMockPassportProof.ts",
