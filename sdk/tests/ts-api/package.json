    "@selfxyz/common": "workspace:^",
    "@selfxyz/core": "workspace:^",
    "asn1.js": "5.4.1",
    "axios": "^1.7.7",
    "cbor": "10.0.11",
    "cors": "^2.8.5",
    "cose-js": "0.9.0",
    "elliptic": "^6.5.7",
    "ethers": "^6.13.5",
    "express": "^4.18.2",
    "socket.io-client": "^4.8.1",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/cose-js": "^0",
    "@types/elliptic": "^6.4.18",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@types/ws": "^8",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=22 <23"
  }
}
